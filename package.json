    "@angular/platform-browser-dynamic": "~12.2.10",
    "@angular/router": "~12.2.10",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@types/jquery": "^3.5.7",
    "angular-file-uploader": "^7.1.0",
    "bootstrap": "^5.1.3",
